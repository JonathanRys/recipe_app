# Auto detect text files and perform LF normalization
*        text

*.html   text diff=html
*.css    text
*.js     text
*.sql    text

*.docx   diff=astextplain
*.DOCX   diff=astextplain

*.png    binary
*.ico    binary

*.py                -text eol=lf
*.sh                -text eol=lf
*.conf              -text eol=lf
*.service           -text eol=lf
requirements.txt    -text eol=lf
recipe_app.ini      -text eol=lf

/config      -text eol=lf
/recipe_app* -text eol=lf

/recipe_app/app/static/img binary